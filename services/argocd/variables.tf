variable "github_username" {
  description = "Github username variable for initial bootstrap, sourced from environment"
  default     = ""
}

variable "github_token" {
  description = "Github token variable for initial bootstrap, sourced from environment"
  default     = ""
}

variable "github_oauth_clientID" {
  description = "Github OAuth Client ID variable for initial bootstrap, sourced from environment"
  default     = ""
}

variable "github_oauth_clientSecret" {
  description = "Github OAuth Client Secret variable for initial bootstrap, sourced from environment"
  default     = ""
}
